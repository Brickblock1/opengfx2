// Climate-sensitive RAILTYPE_TUNNELS
if (climate == CLIMATE_TROPICAL) {
	replacenew (RAILTYPE_TUNNELS, "../graphics/stations/general/64/pygen/tunnels_none_tropical_grass_8bpp.png", 0) { template_tunnels_general(0, 0, 1) }
	replacenew (RAILTYPE_TUNNELS, "../graphics/stations/general/64/pygen/tunnels_none_tropical_desert_8bpp.png", 8) { template_tunnels_general(0, 0, 1) }
} else {
	replacenew (RAILTYPE_TUNNELS, "../graphics/stations/general/64/pygen/tunnels_none_temperate_grass_8bpp.png", 0) { template_tunnels_general(0, 0, 1) }
	replacenew (RAILTYPE_TUNNELS, "../graphics/stations/general/64/pygen/tunnels_none_arctic_snow_8bpp.png", 8) { template_tunnels_general(0, 0, 1) }
	if (climate == CLIMATE_ARCTIC) {
		replacenew (RAILTYPE_TUNNELS, "../graphics/stations/general/64/pygen/tunnels_none_arctic_grass_8bpp.png", 0) { template_tunnels_general(0, 0, 1) }
	}
	if (climate == CLIMATE_TOYLAND) {
		replacenew (RAILTYPE_TUNNELS, "../graphics/stations/general/64/pygen/tunnels_none_toyland_grass_8bpp.png", 0) { template_tunnels_general(0, 0, 1) }
	}
}
